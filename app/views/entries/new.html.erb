<%= form_for(Entry.new) do |form| %>
    <p><%= form.label("title") %></p>
    <p><%= form.text_field("title") %></p>
    <p><%= form.label("Hype link") %></p>
    <p><%= form.text_area("link") %></p>
    <p><%= form.submit %></p>
<% end %>

<p><%= link_to("Back to all entries", entries_path) %></p>